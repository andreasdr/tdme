<screen id="modelviewer" background-color="#00000000">
	<layout alignment="vertical" width="100%" height="100%">
		<panel alignment="none" width="100%" height="30" padding="5" vertical-align="center">
			<text id="screen_caption" font="resources/gui/fonts/Foo_20.fnt" text="Particle System" width="100%" horizontal-align="center" height="auto" />
		</panel>
		<element width="100%" height="*" border="1" border-color="#000000" focusable="true" ignore-events="true" />
		<panel width="100%" height="200">
			<tabs alignment="vertical" width="100%" height="100%">
				<tabs-header>
					<tab id="tab_properties_display" text="Display" />
					<tab id="tab_properties_particlesystem" text="Particle System" />
					<tab id="tab_properties" text="Properties" />
					<tab id="tab_properties_emitter" text="Emitter" />
					<tab id="tab_properties_type" text="Type" />
				</tabs-header>
				<tabs-content>
					<tab-content tab-id="tab_properties_display" vertical-align="center">
						<layout width="auto" height="auto" alignment="vertical">
							<layout width="auto" height="auto" alignment="horizontal" vertical-align="center">
								<checkbox id="display_ground" name="display_ground" value="1" />
								<space width="5" />
								<text font="resources/gui/fonts/Foo_20.fnt" text="Ground" width="120" height="auto"/>
								<space width="20" />
								<checkbox id="display_shadowing" name="display_shadowing" value="1" />
								<space width="5" />
								<text font="resources/gui/fonts/Foo_20.fnt" text="Shadowing" width="120" height="auto"/>
							</layout>
							<space height="5" />
							<layout width="auto" height="auto" alignment="horizontal" vertical-align="center">
								<checkbox id="display_boundingvolume" name="display_boundingvolume" value="1" />
								<space width="5" />
								<text font="resources/gui/fonts/Foo_20.fnt" text="Bounding Volume" width="auto" height="auto"/>
							</layout>
							<space height="20" />
							<layout width="100%" height="auto" alignment="none" horizontal-align="center">
								<button id="button_display_apply" name="button_display_apply" text="Apply" />
							</layout>
						</layout>
					</tab-content>
					<tab-content tab-id="tab_properties_particlesystem" vertical-align="center">
						<layout width="auto" height="auto" alignment="vertical">
							<layout width="auto" height="auto" alignment="horizontal" vertical-align="center">
								<text font="resources/gui/fonts/Foo_20.fnt" text="Name" width="200" height="auto"/>
								<space width="5" />
								<input id="entity_name" name="entity_name" width="200" height="auto" text="Name" />
							</layout>
							<space height="5" />
							<layout width="auto" height="auto" alignment="horizontal" vertical-align="center">
								<text font="resources/gui/fonts/Foo_20.fnt" text="Description" width="200" height="auto"/>
								<space width="5" />
								<input id="entity_description" name="entity_description" width="200" height="auto" text="Description" />
							</layout>
							<space height="20" />
							<layout width="100%" height="auto" alignment="horizontal">
								<space width="*" />
								<button id="button_entity_apply" name="button_model_apply" text="Apply" />
								<space width="*" />
							</layout>
						</layout>
					</tab-content>
					<tab-content tab-id="tab_properties" vertical-align="center">
						<layout width="auto" height="auto" alignment="horizontal">
							<layout width="auto" height="auto" alignment="vertical">
								<layout width="auto" height="auto" alignment="horizontal">
									<dropdown id="entity_properties_presets" name="entity_properties_presets" width="510" height="auto" text="Test">
										<dropdown-option text="Test 1" value="1" />										
									</dropdown>
									<space width="10" />
