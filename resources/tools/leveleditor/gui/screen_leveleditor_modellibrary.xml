<screen id="leveleditor_modellibrary" background-color="#00000000">
	<layout alignment="vertical" width="100%" height="100%">
		<layout alignment="none" width="100%" height="30" padding="5" horizontal-align="center" vertical-align="center">
		</layout>
		<layout width="100%" height="*" alignment="horizontal">
				<layout width="100%" height="100%" alignment="vertical">
					<space height="*" />
					<layout width="100%" height="80%" alignment="horizontal">
						<space width="*" />
						<panel width="180" height="100%">
							<layout alignment="vertical" width="100%" height="100%">
								<selectbox id="model_library_listbox" name="model_library_listbox" width="100%" height="*">
									<scrollarea-vertical id="model_library_listbox_scrollarea" width="100%" height="100%">
										<selectbox-option text="Option A" value="1" />
										<selectbox-option text="Option B" value="2" />
										<selectbox-option text="Option C" value="3" />
										<selectbox-option text="Option D" value="4" />
										<selectbox-option text="Option E" value="5" />
										<selectbox-option text="Option F" value="6" />
										<selectbox-option text="Option G" value="7" />
										<selectbox-option text="Option H" value="8" />
										<selectbox-option text="Option I" value="9" />
										<selectbox-option text="Option J" value="10" selected="true" />
										<selectbox-option text="Option K" value="11" />
										<selectbox-option text="Option L" value="12" />
										<selectbox-option text="Option M" value="13" />
										<selectbox-option text="Option N" value="14" />
										<selectbox-option text="Option O" value="15" />
										<selectbox-option text="Option P" value="16" />
										<selectbox-option text="Option Q" value="17" />
										<selectbox-option text="Option R" value="18" />
										<selectbox-option text="Option S" value="19" />
										<selectbox-option text="Option T" value="20" />
										<selectbox-option text="Option U" value="21" />
										<selectbox-option text="Option V" value="22" />
										<selectbox-option text="Option W" value="23" />
										<selectbox-option text="Option X" value="24" />
										<selectbox-option text="Option Y" value="25" />
										<selectbox-option text="Option Z" value="26" />
									</scrollarea-vertical>
								</selectbox>
								<space height="10" />
								<layout width="100%" height="auto" alignment="horizontal">
									<space width="*" />
									<button id="button_model_library_place" name="button_model_place" text="Place" />
									<space width="*" />
									<button id="button_model_library_edit" name="button_model_library" text="Library" />
									<space width="*" />
								</layout>
								<space height="10" />
							</layout>
						</panel>
					</layout>
					<space height="*" />
				</layout>
		</layout>
		<layout width="100%" height="200">
		</layout>
	</layout>
</screen>